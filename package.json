{
  "name": "bun_nltk",
  "version": "0.6.0",
  "description": "High-performance NLP primitives for Bun/Node with Zig native and WASM backends.",
  "module": "index.ts",
  "type": "module",
  "private": false,
  "license": "Apache-2.0",
  "files": [
    "index.ts",
    "src",
    "scripts",
    "models",
    "zig",
    "README.md",
    "CHANGELOG.md",
    "docs",
    "package.json"
  ],
  "keywords": [
    "nlp",
    "nltk",
    "zig",
    "bun",
    "node",
    "wasm",
    "tokenizer",
    "pos-tagger"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Seyamalam/bun_nltk"
  },
  "homepage": "https://github.com/Seyamalam/bun_nltk#readme",
  "bugs": {
    "url": "https://github.com/Seyamalam/bun_nltk/issues"
  },
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "build:zig": "bun run scripts/build-zig.ts",
    "build:wasm": "bun run scripts/build-wasm.ts",
    "model:train:tagger": "python scripts/train_perceptron_tagger.py --out models/perceptron_tagger_ascii.json --sentences 7000 --epochs 10 --seed 1337",
    "test": "bun test",
    "bench:generate": "python bench/generate_synthetic.py --size-mb 64 --seed 1337 --out bench/datasets/synthetic.txt",
    "bench:compare": "bun run bench/compare.ts bench/datasets/synthetic.txt 2 5",
    "bench:compare:collocations": "bun run bench/compare_collocations.ts bench/datasets/synthetic.txt 50 5",
    "bench:compare:porter": "bun run bench/compare_porter.ts bench/datasets/synthetic.txt 3",
    "bench:compare:wasm": "bun run bench/compare_wasm.ts bench/datasets/synthetic.txt 2 5",
    "bench:compare:sentence": "bun run bench/compare_sentence.ts bench/datasets/synthetic.txt 5",
    "bench:compare:tagger": "bun run bench/compare_tagger.ts bench/datasets/synthetic.txt 1",
    "bench:parity:sentence": "bun run bench/parity_sentence.ts",
    "bench:parity:tagger": "bun run bench/parity_tagger.ts",
    "bench:gate": "bun run scripts/bench-gate.ts",
    "bench:dashboard": "bun run scripts/bench-dashboard.ts",
    "npm:smoke": "bun run scripts/npm-smoke-test.ts",
    "release:validate": "bun run scripts/release-validate.ts",
    "release:check": "bun run build:zig && bun run build:wasm && bun test && bun run bench:gate"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
