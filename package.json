{
  "name": "bun_nltk",
  "version": "0.9.0",
  "description": "High-performance NLP primitives for Bun/Node with Zig native and WASM backends.",
  "module": "index.ts",
  "type": "module",
  "private": false,
  "license": "Apache-2.0",
  "files": [
    "index.ts",
    "native/prebuilt/linux-x64/bun_nltk.so",
    "native/prebuilt/win32-x64/bun_nltk.dll",
    "native/bun_nltk.wasm",
    "src",
    "corpora",
    "scripts",
    "models",
    "zig",
    "README.md",
    "CHANGELOG.md",
    "docs",
    "package.json"
  ],
  "keywords": [
    "nlp",
    "nltk",
    "zig",
    "bun",
    "node",
    "wasm",
    "tokenizer",
    "pos-tagger"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Seyamalam/bun_nltk"
  },
  "homepage": "https://github.com/Seyamalam/bun_nltk#readme",
  "bugs": {
    "url": "https://github.com/Seyamalam/bun_nltk/issues"
  },
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "build:zig": "bun run scripts/build-zig.ts",
    "build:prebuilt": "bun run scripts/build-prebuilt.ts",
    "build:wasm": "bun run scripts/build-wasm.ts",
    "model:train:tagger": "python scripts/train_perceptron_tagger.py --out models/perceptron_tagger_ascii.json --sentences 7000 --epochs 10 --seed 1337",
    "fixtures:import:nltk": "python scripts/import_nltk_fixtures.py --out-dir test/fixtures/nltk_imported --max-trees 220 --max-parser-cases 80 --train-per-label 220 --test-per-label 80",
    "wordnet:pack": "bun run scripts/pack-wordnet.ts --dict-dir ./wordnet/dict --out models/wordnet_full.bin --checksum-out models/wordnet_full.sha256.json",
    "wordnet:pack:official": "bun run scripts/pack-wordnet-official.ts --out artifacts/wordnet_official.bin --checksum-out artifacts/wordnet_official.sha256.json",
    "wordnet:verify:pack": "bun run scripts/verify-wordnet-pack.ts",
    "test": "bun test",
    "bench:generate": "python bench/generate_synthetic.py --size-mb 64 --seed 1337 --out bench/datasets/synthetic.txt",
    "bench:compare": "bun run bench/compare.ts bench/datasets/synthetic.txt 2 5",
    "bench:compare:collocations": "bun run bench/compare_collocations.ts bench/datasets/synthetic.txt 50 5",
    "bench:compare:porter": "bun run bench/compare_porter.ts bench/datasets/synthetic.txt 3",
    "bench:compare:wasm": "bun run bench/compare_wasm.ts bench/datasets/synthetic.txt 2 5",
    "bench:compare:sentence": "bun run bench/compare_sentence.ts bench/datasets/synthetic.txt 5",
    "bench:compare:punkt": "bun run bench/compare_punkt.ts bench/datasets/synthetic.txt 5",
    "bench:compare:tagger": "bun run bench/compare_tagger.ts bench/datasets/synthetic.txt 1",
    "bench:compare:freqdist": "bun run bench/compare_freqdist_stream.ts bench/datasets/synthetic.txt 2048",
    "bench:compare:lm": "bun run bench/compare_lm.ts bench/datasets/synthetic.txt 3",
    "bench:compare:chunk": "bun run bench/compare_chunk.ts 15000 5",
    "bench:compare:wordnet": "bun run bench/compare_wordnet.ts 8",
    "bench:compare:parser": "bun run bench/compare_parser.ts 800 4",
    "bench:compare:classifier": "bun run bench/compare_classifier.ts 2400 600 4",
    "bench:compare:linear": "bun run bench/compare_linear_scores.ts 6000 12000 6 40 5",
    "bench:compare:linear-train": "bun run bench/compare_linear_training_native_vs_js.ts 6000 1200 3",
    "bench:compare:decision-tree": "bun run bench/compare_decision_tree.ts 2400 600 3",
    "bench:compare:earley": "bun run bench/compare_earley.ts 2000 3",
    "bench:compare:leftcorner": "bun run bench/compare_leftcorner.ts 1200 3",
    "bench:compare:feature-parser": "bun run bench/compare_feature_parser.ts 1200 3",
    "bench:compare:pcfg": "bun run bench/compare_pcfg.ts 700 3",
    "bench:compare:maxent": "bun run bench/compare_maxent.ts 900 250 2",
    "bench:compare:condexp": "bun run bench/compare_condexp.ts 1000 300 2",
    "bench:compare:positive-nb": "bun run bench/compare_positive_nb.ts 800 2400 500 3",
    "bench:compare:simd": "bun run bench/compare_simd.ts bench/datasets/synthetic.txt 5",
    "bench:browser:wasm": "bun run bench/browser_wasm.ts",
    "bench:parity:tokenizer": "bun run bench/parity_tokenizer.ts",
    "bench:parity:sentence": "bun run bench/parity_sentence.ts",
    "bench:parity:punkt": "bun run bench/parity_punkt.ts",
    "bench:parity:wordnet": "bun run bench/parity_wordnet.ts",
    "bench:parity:parser": "bun run bench/parity_parser.ts",
    "bench:parity:classifier": "bun run bench/parity_classifier.ts",
    "bench:parity:pcfg": "bun run bench/parity_pcfg.ts",
    "bench:parity:maxent": "bun run bench/parity_maxent.ts",
    "bench:parity:decision-tree": "bun run bench/parity_decision_tree.ts",
    "bench:parity:earley": "bun run bench/parity_earley.ts",
    "bench:parity:leftcorner": "bun run bench/parity_leftcorner.ts",
    "bench:parity:feature-parser": "bun run bench/parity_feature_parser.ts",
    "bench:parity:corpus-imported": "bun run bench/parity_corpus_imported.ts",
    "bench:parity:imported": "bun run bench/parity_imported.ts",
    "bench:parity:tagger": "bun run bench/parity_tagger.ts",
    "bench:parity:condexp": "bun run bench/parity_condexp.ts",
    "bench:parity:positive-nb": "bun run bench/parity_positive_nb.ts",
    "bench:parity:all": "bun run bench/parity_all.ts",
    "parity:report": "bun run scripts/parity-report.ts",
    "parity:tracker": "bun run scripts/parity-tracker.ts",
    "wasm:size:check": "bun run scripts/wasm-size-check.ts",
    "sla:gate": "bun run scripts/sla-gate.ts",
    "bench:gate": "bun run scripts/bench-gate.ts && bun run sla:gate && bun run bench:parity:all",
    "bench:dashboard": "bun run scripts/bench-dashboard.ts",
    "bench:trend:check": "bun run scripts/bench-trend-check.ts",
    "bench:trend:record": "bun run scripts/bench-trend-record.ts",
    "npm:smoke": "bun run scripts/npm-smoke-test.ts",
    "pack:verify:prebuilt": "bun run scripts/check-packaged-prebuilt.ts",
    "release:validate": "bun run scripts/release-validate.ts",
    "release:check": "bun run build:zig && bun run build:wasm && bun run build:prebuilt && bun run pack:verify:prebuilt && bun run fixtures:import:nltk && bun test && bun run bench:gate && bun run bench:dashboard && bun run bench:trend:check && bun run wordnet:pack:official && bun run wordnet:verify:pack"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "playwright": "^1.52.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
